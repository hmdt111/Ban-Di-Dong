@model IEnumerable<Ban_Di_Dong.Areas.Admin.Models.SupplierAdminVM>

@{
    ViewData["Title"] = "DanhMucNhaCungCap";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<h1>DANH MỤC NHÀ CUNG CẤP</h1>
<a asp-action="ThemNhaCungCapMoi" class="btn btn-primary">Thêm nhà cung cấp</a>
<hr />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                MÃ
            </th>
            <th>
                TÊN
            </th>
            <th>
                EMAIL
            </th>
            <th>SĐT</th>
            <th>ĐỊA CHỈ</th>
            <th>CÔNG CỤ</th>
           

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SupplierId</td>
                <td>@item.SupplierName</td>
                <td>@item.SupplierEmail</td>
                <td>@item.SupplierPhone</td>
                <td>@item.SupplierAddress</td>
                <td>
                    <a asp-action="ThemLoaiSanPhamMoi" class="btn btn-primary">Chỉnh Sửa</a>
                    <a asp-action="ThemLoaiSanPhamMoi" class="btn btn-primary">Xoá</a>

                </td>
                
            </tr>
        }

    </tbody>
</table>




